#include <stdio.h>
#include <stdlib.h>

void integrarElemento(char array[], char dato, int validos);

int main() {

int i,validos;

char array[10] = {'a','b','c','g','h','k', 'o'};
char dato;

validos = 7;

printf("Ingresar caracter para agregar al array: ");
scanf("%c", &dato);


integrarElemento(array, dato, validos);


for(i = 0; i <= (validos + 1); i++) {

    printf("%c", array[i]);
}


return 0;
}

void integrarElemento(char array[], char dato, int validos) {
int i = 0;

if (dato > array[(validos -1)]){

    array[validos] = dato;
}
else if(dato < array[(validos - 1)]) {

    while(dato < array[(validos - 1)]){

    array[validos] = array[validos - 1];
    validos--;
}
array[validos] = dato;

}
}
