#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num1, num2, num3;



    printf("Dame 3 numeros\n");
    scanf("%i", &num1);
    scanf("%i", &num2);
    scanf("%i", &num3);


    if(num1 > num2 && num1 >num3) {
        printf("numero: %i", num1);
    }
    else if(num2 > num1 && num2 > num3){
        printf("numero: %i", num2);
    }
    else {
        printf("numero: %i", num3);
    }
}
