#include <stdio.h>
#include <stdlib.h>
#include "pila.h"

int main()
{
Pila pilaA, pilaB, aux;

inicpila(&pilaA);
inicpila(&pilaB);
inicpila(&aux);


apilar(&pilaA, 1);
apilar(&pilaA, 2);
apilar(&pilaA, 3);

apilar(&pilaB, 2);
apilar(&pilaB, 3);
apilar(&pilaB, 5);
apilar(&pilaB, 7);

mostrar(&pilaB);
mostrar(&pilaA);
/// funciona


  while(!pilavacia(&pilaA) && pilavacia(&aux)) {

        if(tope(&pilaA) == tope(&pilaB)){

        desapilar(&pilaA);
        }

        else{

    while(tope(&pilaA) != tope(&pilaB)){

     apilar(&aux, desapilar(&pilaB));
    }

       if(pilavacia(&pilaB)) {
            apilar(&pilaB, desapilar(&pilaA));

    while(!pilavacia(&aux)) {
        apilar(&pilaB, desapilar(&aux));
    }
    }
}
}
mostrar(&pilaB);
mostrar(&pilaA);
mostrar(&aux);


return 0;
}
